package es.cristian.modelo.entidad;

public class MensajeEncriptado {
	
	private int id;
	private byte[] mensaje;
	
	public MensajeEncriptado(int id, byte[] mensaje) {
		super();
		this.id = id;
		this.mensaje = mensaje;
	}
	
	
	
	public MensajeEncriptado() {
		
	}



	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public byte[] getMensaje() {
		return mensaje;
	}
	public void setMensaje(byte[] mensaje) {
		this.mensaje = mensaje;
	}



	@Override
	public String toString() {
		return "MensajeEncriptado [id=" + id + ", mensaje=" + mensaje + "]";
	}



	
	

}
